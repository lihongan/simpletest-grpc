{
    "apiVersion": "v1",
    "kind": "List",
    "items": [
        {
            "apiVersion": "v1",
            "kind": "ReplicationController",
            "metadata": {
                "labels": {
                    "name": "grpc-rc"
                },
                "name": "grpc-rc"
            },
            "spec": {
                "replicas": 1,
                "template": {
                    "metadata": {
                        "labels": {
                            "name": "grpcserver-pods"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "image": "lihongan/grpc-server",
                                "name": "grpc-server"
                            }
                        ]
                    }
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "labels": {
                    "name": "service-secure"
                },
                "name": "service-secure"
            },
            "spec": {
                "ports": [
                    {
                        "name": "https",
                        "port": 50080,
                        "protocol": "TCP",
                        "targetPort": 50080
                    }
                ],
                "selector": {
                    "name": "grpcserver-pods"
                }
            }
        },
       {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "labels": {
                    "name": "service-unsecure"
                },
                "name": "service-unsecure"
            },
            "spec": {
                "ports": [
                    {
                        "name": "http",
                        "port": 50081,
                        "protocol": "TCP",
                        "targetPort": 50081
                    }
                ],
                "selector": {
                    "name": "grpcserver-pods"
                }
            }
        }

    ]
}

